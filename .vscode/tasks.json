{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "wrangler dev",
      "type": "shell",
      "command": "bun run dev",
      "isBackground": true,
      "problemMatcher": {
        "owner": "wrangler",
        "pattern": {
          "regexp": ".*",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": ".*"
        }
      }
    }
  ]
}
